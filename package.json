{
  "name": "gitee-pages",
  "version": "1.0.0",
  "description": "GitHub Actions for Gitee Pages",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.20.1",
    "npm": ">=7.5.2"
  },
  "scripts": {
    "postinstall": "npx husky install",
    "all": "npm run format && npm run lint && npm test",
    "lint": "eslint ./{src,__tests__}/**/*.ts",
    "lint:fix": "eslint --fix ./{src,__tests__}/**/*.ts",
    "test": "jest --coverage --verbose --detectOpenHandles",
    "build": "ncc build ./src/index.ts -o lib --minify",
    "tsc": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'",
    "release": "standard-version"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "prettier --check",
      "eslint"
    ],
    "README.md": [
      "npx doctoc@2.1.0 --github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimmy214/gitee-pages.git"
  },
  "keywords": [
    "GitHub Actions",
    "Actions",
    "JavaScript Action",
    "TypeScript Action",
    "Gitee Pages",
    "gh-pages"
  ],
  "author": "peaceiris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kimmy214/gitee-pages/issues"
  },
  "homepage": "https://github.com/kimmy214/gitee-pages#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@types/shelljs": "^0.8.11",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/js-yaml": "4.0.5",
    "@types/node": "12.20.55",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "js-yaml": "4.1.0",
    "lint-staged": "13.3.0",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "ts-jest": "26.5.6",
    "typescript": "4.8.4"
  }
}
